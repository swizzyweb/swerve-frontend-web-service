{
  "name": "@swizzyweb/swerve-frontend-web-service",
  "version": "0.1.5",
  "description": "Swerve frontend web service",
  "main": "dist/app.js",
  "author": "Jason Gallagher",
  "type": "module",
  "scripts": {
    "dev": "npm run build && npm run server",
    "server": "swerve",
    "build:service": "tsc",
    "build": "npm run build:webpack && npm run copy:public && npm run build:service",
    "start:webpack": "webpack serve --config webpack.config.cjs",
    "build:webpack": "webpack --config webpack.config.cjs",
    "copy:public": "cp -r ./public/* bundle",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs": "npx typedoc src/index.ts"
  },
  "keywords": [
    "react",
    "webpack",
    "tailwind",
    "route-optimizer",
    "typescript"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@swizzyweb/swerve": "^0.6.2",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/express": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "eslint": "^9.39.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@swizzyweb/express-unuse": "^1.0.1",
    "@swizzyweb/swerve-manager": "^0.3.2",
    "@swizzyweb/swizzy-common": "^0.3.3",
    "@swizzyweb/swizzy-web-service": "^0.6.1",
    "express": "^5.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
